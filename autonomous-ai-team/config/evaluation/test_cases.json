  "quality_criteria": {
    "contains_specific_numbers": {
      "description": "Output contains concrete numbers, not vague estimates",
      "regex": "\\d+[%$kMB]",
      "min_matches": 3
    },
    "urls_are_valid": {
      "description": "All URLs are properly formatted",
      "regex": "https?://[^\\s]+",
      "validate_format": true
    },
    "no_placeholder_text": {
      "description": "No placeholder text like [TODO], [FILL IN], etc.",
      "regex": "\\[(TODO|FILL|TBD|XXX).*?\\]",
      "should_not_match": true
    },
    "actionable_recommendations": {
      "description": "Contains clear next steps or recommendations",
      "keywords": ["next step", "recommend", "action", "should", "start with"],
      "min_matches": 2
    },
    "experiments_are_testable": {
      "description": "Growth experiments have clear hypothesis and metrics",
      "keywords": ["hypothesis", "metric", "test", "measure"],
      "min_matches": 3
    },
    "metrics_are_specific": {
      "description": "Metrics are specific with targets",
      "keywords": ["increase", "decrease", "reach", "achieve"],
      "requires_numbers": true
    },
    "headline_under_15_words": {
      "description": "Headline is concise (under 15 words)",
      "section": "headline",
      "max_words": 15
    },
    "cta_is_specific": {
      "description": "CTA is action-oriented and specific",
      "keywords": ["start", "get", "try", "download", "join"],
      "section": "CTA"
    },
    "benefits_not_features": {
      "description": "Focuses on benefits/outcomes, not just features",
      "benefit_keywords": ["save", "reduce", "increase", "achieve", "get"],
      "min_benefit_matches": 3
    },
    "roi_is_calculated": {
      "description": "ROI or payback period is explicitly calculated",
      "keywords": ["ROI", "payback", "return on investment", "savings"],
      "requires_numbers": true
    },
    "tools_are_specific": {
      "description": "Recommends specific tools by name",
      "examples": ["Zapier", "Make", "HubSpot", "Intercom"],
      "min_tool_mentions": 2
    },
    "keywords_are_specific": {
      "description": "SEO keywords are specific, not generic",
      "min_keywords": 5,
      "min_keyword_length": 3
    }
  },
  "performance_benchmarks": {
    "response_time": {
      "excellent": 30,
      "good": 60,
      "acceptable": 120,
      "poor": 180
    },
    "token_efficiency": {
      "input_tokens_per_task": {
        "simple": 1500,
        "medium": 3000,
        "complex": 6000
      },
      "output_tokens_per_task": {
        "simple": 1000,
        "medium": 2000,
        "complex": 4000
      }
    },
    "cost_targets": {
      "simple_task": 0.05,
      "medium_task": 0.15,
      "complex_task": 0.40,
      "multi_agent_task": 1.00
    }
  }
}
